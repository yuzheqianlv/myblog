{
    "buildCommand": "bash build.sh",
    "outputDirectory": "public",
    "installCommand": "curl --proto '=https' --tlsv1.2 -sSf https://sh.rustup.rs | sh -s -- -y && source $HOME/.cargo/env",
    "framework": null,
    "routes": [
        {
            "src": "/api/search",
            "dest": "/api/search"
        }
    ],
    "functions": {
        "api/search": {
            "runtime": "vercel-rust@4.0.0"
        }
    }
} 